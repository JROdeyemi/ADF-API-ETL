{
	"name": "FetchAPIData",
	"properties": {
		"activities": [
			{
				"name": "Copy Data from API",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings",
							"copyBehavior": "FlattenHierarchy"
						},
						"formatSettings": {
							"type": "JsonWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "SpotonAPI_dataset",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "raw_spoton_data_json",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "ForEach1",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.locationList",
						"type": "Expression"
					}
				}
			}
		],
		"parameters": {
			"LocationList": {
				"type": "array",
				"defaultValue": [
					"Location16075",
					"Location16672",
					"Location19170",
					"Location19150",
					"Location19174"
				]
			}
		},
		"annotations": []
	}
}